 		}		
 	}
	
	var sectionWidth = this.ds_combosetMat.getColumn(nRow, "sectionWidth");	// 단면폭
	var aspectRatio  = this.ds_combosetMat.getColumn(nRow, "aspectRatio");	// 편평비
