	this._setReturnRentalArea = function(val){
		var sDsNm = val[0];
		var nRowCnt = sDsNm.rowcount;
		
		if ( nRowCnt > 0)
		{
			this.dsListArea.clearData();
			this.dsListArea.addRow();
			this.dsListArea.copyRow(0, sDsNm, 0); 
		}
	}	

