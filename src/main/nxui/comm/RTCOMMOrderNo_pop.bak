			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));
		}else if(this.parent.p_arg == "RTSDCustCondition"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));	
		}else if(this.parent.p_arg == "RTRECollectionMoney"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));	
