		}else if( frCd == "" ){
			alert("전후 구분 값이 없습니다.");
			return false;
		}else if( cnt == "" ){
			alert("타이어본수 구분 값이 없습니다.");
			this.div_search.cb_searchCnt.setFocus();
			return false;
		}
	}
	
	var sSvcID        	= "listProductPop";                    
	var sController   	= "rtms/comm/listProductPop.do";
	var sInDatasets   	= "";
	var sOutDatasets  	= "ds_product=mapProduct";
	var sArgs 			= "";	
