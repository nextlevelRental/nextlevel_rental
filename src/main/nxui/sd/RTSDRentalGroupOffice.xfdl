		var nCol = e.col;
		if(nCol == 9 || nCol == 10)
		{
			var args ={p_argGrid:"1"};
			Ex.core.popup(this,"렌탈지역찾기(Grid)","sd::RTSDRentalAreaRegisterPopup.xfdl",args, "modaless=false");			
		}		
