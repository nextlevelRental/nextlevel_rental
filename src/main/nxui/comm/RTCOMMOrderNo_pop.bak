/*|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||*/
this.FN_choice = function(){
	var pos = this.ds_ordNoList.rowposition;
	if( pos == -1 ){
		alert("선택된 행이 없습니다.");
	}else{
		if(this.parent.p_arg == "RTSDPlan"){ //장착등록 RTSDPlan.xfdl
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTSDOrder") { //주문현황리스트 RTSDOrder.xfdl
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTSDClaim") {
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTSDContractRegister") {
			var ordNo 	 = this.ds_ordNoList.getColumn(pos, "ordNo");
			var birthDay = this.ds_ordNoList.getColumn(pos, "birthDay");
			var telNo  	 = this.ds_ordNoList.getColumn(pos, "telNo");
			this.close(this.opener.returnOrderNoInfo(ordNo, birthDay, telNo));
		}else if(this.parent.p_arg == "RTSDContractRegister"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTSDContractRegister"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTREDeferSelect"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTCSRegularCheck"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNoInfo(ordNo));
		}else if(this.parent.p_arg == "RTCSKeepHistory" || this.parent.p_arg =="RTCSEngineOilRepSer" || this.parent.p_arg=="RTCSZeroSed_makerNmervice"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));
		}else if(this.parent.p_arg == "RTCOMMStrgRegister"){
			var ordNo		= nvl(this.ds_ordNoList.getColumn(pos, "ordNo"));
			var carKind		= nvl(this.ds_ordNoList.getColumn(pos, "carKind"));
			var telNo		= nvl(this.ds_ordNoList.getColumn(pos, "telNo"));
			var custNo		= nvl(this.ds_ordNoList.getColumn(pos, "custNo"));
			var custNm		= nvl(this.ds_ordNoList.getColumn(pos, "custNm"));
			var carNo		= nvl(this.ds_ordNoList.getColumn(pos, "carNo"));
			var mobNo		= nvl(this.ds_ordNoList.getColumn(pos, "mobNo"));
			var seasonCd	= nvl(this.ds_ordNoList.getColumn(pos, "seasonCd"));
			var statCd 	 	= nvl(this.ds_ordNoList.getColumn(pos, "statCd"));
			
			if( (statCd != "03") && (statCd != "04") ){
				alert("보관등록을 할 수 없는 계약상태입니다.");
				return false;
			}else if( seasonCd != "02" ){
				alert("계절 구분 겨울용이 아니므로 보관 등록을 할 수가 없습니다.");
				return false;
			}else{
				var arr = [ ordNo, carKind, telNo, custNo, custNm, carNo, mobNo, seasonCd ];
				this.close(this.opener.returnOrderNo(arr));
			}
		}else if(this.parent.p_arg == "RTCMOrdAgentOrder"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));
		}else if(this.parent.p_arg == "RTCMOrdAgentOrder"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));
		}else if(this.parent.p_arg == "RTREArrears"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));
		}else if(this.parent.p_arg == "RTREArrearsResult"){
			var ordNo = this.ds_ordNoList.getColumn(pos, "ordNo");
			this.close(this.opener.returnOrderNo(ordNo));	
