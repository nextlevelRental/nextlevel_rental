this.btn_delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	var delRow = -1;
	var cnt = this.ds_rtcs0202.getRowCount();
	for( var i = 0 ; i < cnt ; i++ ){
		if( this.ds_rtcs0202.getRowType(i) == 2 ){
			delRow = i;
		}else{
			delRow = -1
		}
	}
	
	if( delRow != -1 ){
		this.ds_rtcs0202.deleteRow(delRow);
	}else{
		alert("삭제할 행이 없습니다.");
	}
}
