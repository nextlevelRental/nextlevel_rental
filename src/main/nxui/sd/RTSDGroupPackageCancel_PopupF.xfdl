/***********************************************************************************
* 5. CallBack Functions
***********************************************************************************/	
	/***********************************************************************************
	* CallBack Event (strSvcId - Sevice ID, nErrorCode - ErrorCode, strErrorMsg - Error Message)
	***********************************************************************************/

	/* callBack함수 */
	this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
	{ 
		if (nErrorCode < 0) {
			alert(strErrorMsg);		
		}
		
		if (strSvcId == "listCommInfo") { //공통코드
			this.fn_search();
		}else if (strSvcId == "listGroupInfo") { //단체정보 조회
			var nRow = this.dsList.rowcount;
			this.div_search.edt_grpNo00.set_value(this.dsList.getColumn(0,"dcCd"));
// 			for(var i=0;i<this.ds_restCnt.getRowCount();i++){
// 			if(this.div_search.edt_grpNo00.value==this.ds_restCnt.getColumn(i,"dcCd")){
// 					if(this.ds_restCnt.getColumn(i,"minCnt")<=this.div_search.edt_grpNo00.value){
// 						
// 					}
// 				}
// 			}
			if( nRow < 1 ){
				alert("조회된 데이터가 없습니다.");
			}else{				
				this.fn_searchContract(); //계약정보
			}
		}else if( strSvcId == "ordNoList"){	//계약정보 조회
			var nRow = this.ds_ordNoList.rowcount;

			if( nRow > 0 ){
				//this.fn_changeComponent(this.ds_ordNoList.getColumn(this.ds_ordNoList.rowposition, "custTp"));//고객유형(S024)별 Componenet변경
				for(var i=0; i<this.ds_ordNoList.rowcount; i++){
					//trace("setColumn :: " + this.ds_ordNoList.setColumn(i, "chk", 0));
				}
			}
		}else if( strSvcId == "cancelOrdNo"){	//해지
			alert(strErrorMsg);			
			this.fn_search();
			this.fn_searchContract();
		}
		if( strSvcId =="cancelPackage"){
			alert(strErrorMsg);
			this.fn_search();
			this.fn_searchContract();
		}
		
		if( strSvcId =="cancelPrice" ){
			if( confirm( "위약금은 " + this.resultAmt +"원 입니다. 해지하시겠습니까?")){
				this.fn_cancelPricePopup();	
			}
		}
	}

