			if( selectionWidth == "" ){ alert("폭" + msg); 		return false;}
			if( aspectRatio == "" ){ 	alert("시리즈" + msg); return false;}
			if( wheelInches == "" ){ 	alert("인치" + msg); 	return false;}
			if( plyRating == "" ){ 		alert("PR" + msg); 		return false;}
			if( tireCnt == "" ){ 		alert("수량" + msg); 	return false;}
