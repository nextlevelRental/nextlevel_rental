		Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs, fn_callBack);
	}
	
	//----------------------------------------------------------------------------------
	// Cancel: 해지 Step1 : Popup
	//----------------------------------------------------------------------------------
	
	this.fn_cancelPricePopup = function(){
	

		var ordNo = this.div_ordNoDetail.ed_ordNo.value;
	/*var matNm = this.ed_SaleProdNm.value;*/
		var custNo = this.div_ordNoDetail.ed_custNo.value;
		var custNm  = this.div_ordNoDetail.ed_custNm.value;
		var bizNo = nvl(this.div_ordNoDetail.ed_custTp.value)=='02'?this.div_ordNoDetail.ed_buslNo.value:this.div_ordNoDetail.ed_birthDay.value;

		var mobNo = this.div_ordNoDetail.ed_mobNo.value;

		var regiAmt = "0";

		var onePymnt = this.resultAmt;
		var p_recpTp	    = "42";
		
		
		if(onePymnt != '0' || onePymnt != 0){	
			var args = {"ordNo": ordNo, "custNm": custNm, "mobNo" :mobNo, "regiAmt" : regiAmt, "onePymnt" : onePymnt, "custNo" : custNo, "bizNo" : bizNo , "recpTp" : p_recpTp};
			Ex.core.popup(this,"결제","re::RTREPrcmAmtNOnePymntRecp_pop.xfdl",args, "modaless=false");	
		}else{
			this.fn_cancel2();
		}			
	}
	
	//----------------------------------------------------------------------------------
	// Cancel: 해지 Step2
	//----------------------------------------------------------------------------------
	this.fn_popupCallback = function(args){
		 Ex.core.popup(this, "KCP 카드즉시출금 결과", "re::RTREPaymentCommCrdResult_pop.xfdl", args, "modaless=false");
		this.fn_cancel2();
	}	
	
	//----------------------------------------------------------------------------------
	// Cancel: 해지 Step3
	//----------------------------------------------------------------------------------
	
	
	this.fn_cancel2 = function(){
	if(!this.fn_cancelValidation())
		{
			return false;
		}
				
		if( confirm( "해지하시겠습니까?"))
		{
			this.ds_ordNoListChk.clearData();
			this.ds_ordNoListChk.copyData(this.ds_ordNoList);
			this.ds_ordNoListChk.addColumn("grpType", "string");			
			var nRow = this.ds_ordNoListChk.rowcount;
			var sGrpType = this.dsList.getColumn(0, "dcGb"); //단체유형
			var nRestCnt = nexacro.toNumber(this.dsList.getColumn(0, "grpCnt")); //잔여계약수
			var nCntCd = 0; //선택한 타이어본수의 합
			
				
			if(sGrpType == "D") //단체
			{				
			}
			else if(sGrpType == "C") //패키지
			{
			this.ds_ordNoListChk.clearData();
			var nListCnt = this.ds_ordNoList.getRowCount();
			for(var i=0;i<this.ds_ordNoList.getRowCount();i++){
				if(this.ds_ordNoList.getColumn(i,"chk")==1){
					this.ds_ordNoListChk.copyRow(this.ds_ordNoListChk.addRow(),this.ds_ordNoList,i);
					this.ds_ordNoListChk.addColumn("grpType", "string");
					this.ds_ordNoListChk.setColumn(i, "grpType", "F");
				}else{
					this.ds_ordNoListChk00.copyRow(this.ds_ordNoListChk00.addRow(),this.ds_ordNoList,i);
				}
			}
			
			var nCnt = this.ds_ordNoListChk.getRowCount();
			var nCntCd = nListCnt - nCnt;
			if(nCntCd >=1)
			{
				for(var i=0;i<this.ds_restCnt.getRowCount();i++){
					if(this.ds_restCnt.getColumn(i,"minCnt")<=nCntCd){
						if(this.ds_restCnt.getColumn(i,"maxCnt")>=nCntCd){
							if(this.div_search.edt_grpNo00.value!=this.ds_restCnt.getColumn(i,"dcCd")){
								this.div_search.edt_grpNo01.set_value(this.ds_restCnt.getColumn(i,"dcCd"));
								this.fn_cancelOrdNo2();
							}
							else{
								this.fn_cancelOrdNo();
							}
						}
					}
				}
			}
			else
			{
				for(var i=0;i<this.ds_ordNoListChk.getRowCount();i++){
					this.ds_ordNoListChk.addColumn("grpSt", "string"); //단체상태 "해지" flag용 변수
					this.ds_ordNoListChk.setColumn(i, "grpSt", "Y");
				}
				this.fn_cancelOrdNo();
				}
			}					
		}
	}
	//----------------------------------------------------------------------------------
	// Cancel: 해지:Step1
	//----------------------------------------------------------------------------------

	this.fn_cancelOrdNo = function(){
	this.ds_ordNoListChk.addColumn("grpType", "string");
	for(var i=0;i<this.ds_ordNoListChk.getRowCount();i++){
		this.ds_ordNoListChk.setColumn(i, "grpType", 'F');
		this.ds_ordNoListChk.setColumn(i,"dcCd",nvl(this.div_search.edt_grpNo00.value));
	}
		var sSvcID        	= "cancelOrdNo";
		var sController   	= "/rtms/sd/cancelOrdNo.do";
		var sInDatasets   	= "";
		var sOutDatasets  	= "";
		var sArgs 			= "";	
		sInDatasets   		= "input=ds_ordNoListChk";
		sOutDatasets  		= "";
		var fn_callBack		= "fn_callBack";
		var bAsync	  = false;							//동기화추가(20190220)
							
		Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs, fn_callBack, true, bAsync);
	}


	//----------------------------------------------------------------------------------
	// Cancel: 해지:Step2
	//----------------------------------------------------------------------------------

	this.fn_cancelOrdNo2 = function(){
	this.ds_ordNoListChk00.addColumn("grpType", "string");
	this.ds_ordNoListChk.addColumn("grpType", "string");
	this.ds_ordNoListChk.addColumn("dcCd", "string");
	for (var i=0; i<this.ds_ordNoListChk00.getRowCount();i++){		
		this.ds_ordNoListChk00.setColumn(i, "grpType", 'F');
		this.ds_ordNoListChk00.setColumn(i,"dcCd", nvl(this.div_search.edt_grpNo01.value));
	}
	
	for (var i=0; i<this.ds_ordNoListChk.getRowCount();i++){		
		this.ds_ordNoListChk.setColumn(i, "grpType", 'F');
		this.ds_ordNoListChk.setColumn(i,"dcCd",nvl(this.div_search.edt_grpNo00.value));
	}

		var sSvcID        	= "cancelPackage";
		var sController   	= "/rtms/sd/cancelPackage.do";
		var sInDatasets   	= "";
		var sOutDatasets  	= "";
		var sArgs 			= "";
		
		sInDatasets   		+= " dsOrdNolistChk=ds_ordNoListChk:A";
		sInDatasets   		+= " dsOrdNolistChk00=ds_ordNoListChk00:A";
		var fn_callBack		= "fn_callBack";
		var bAsync	  = false;					//동기화추가(20190220)
						
		Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs, fn_callBack, true, bAsync);
	}
	

