		return;
	}
	
	var args = {  ordNo :   nvl(this.ds_ordAgentList.getColumn(this.ds_ordAgentList.rowposition, "ordNo")), reqNum : "0" };
	Ex.core.popup(this,"배송조회팝업","comm::RTCOMMDeliveryBoard_pop.xfdl",args, "modaless=false");
}
