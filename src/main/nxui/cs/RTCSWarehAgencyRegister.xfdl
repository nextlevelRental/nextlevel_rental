	var lastDay   =  this.toDay.substr(0,4)+this.toDay.substr(4,2)+Eco.date.getLastDayOfMonth(this.toDay);
	
	if( !(( firstDate <= thisValue ) && (thisValue <= lastDay)) ){
		alert("시작일자는 "+firstDate+" ~ "+lastDay+" 선택가능합니다.");
		obj.set_value(this.toDay);
	}
}
